{
  "name": "rescript-project-template",
  "version": "0.0.1",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "bsc-flags": ["-open JsonCombinators"],
  "suffix": ".bs.js",
  "bs-dependencies": ["@rescript/react", "@glennsl/rescript-json-combinators"],
  "warnings": {
    "error": "+101"
  },
  "jsx": {
    "version": 4,
    "mode": "classic"
  }
}
